model UserChoice {
  createdAt DateTime @default(now())
  userId    String
  choiceId  String
  choice    Choice   @relation(fields: [choiceId], references: [id], onDelete: Cascade)
  user      User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@id([userId, choiceId])
  @@map("user_choices")
}
