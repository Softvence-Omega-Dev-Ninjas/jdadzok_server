model AdRevenueShare {
  id        String   @id @default(uuid())
  userId    String
  month     Int
  year      Int
  amount    Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  user      User     @relation(fields: [userId], references: [id])

  @@unique([userId, month, year])
  @@map("adRevenueShare")
}
