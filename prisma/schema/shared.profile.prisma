model SharedProfile {
  id             String       @id @default(uuid())
  title          String
  bio            String
  avatarUrl      String?
  coverUrl       String?
  location       String?
  followersCount Int          @default(0)
  followingCount Int          @default(0)
  // added new attributes 
  fieldOfWork    String
  About          String
  community      Communities?

  // createdAt DateTime @default(now())
  // updatedAt DateTime @updatedAt

  @@map("sharedProfile")
}
