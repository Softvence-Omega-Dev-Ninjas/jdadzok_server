model ActivityScore {
    id String @id @default(uuid())
    like Float
    comment Float
    share Float
    post Float
    greenCapScore Float
    redCapScore Float
    blackCapScore Float
    yellowCapScore Float
    productSpentPercentage Float @default(0.0)
    productPromotionPercentage Float @default(0.0)
    
    greenPercentage Float @default(0.0)
    yellowPercentage Float @default(0.0)
    blackPercentage Float @default(0.0)
    redPercentage Float @default(0.0)
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
