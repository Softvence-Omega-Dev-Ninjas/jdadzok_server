model SellerEarnings {
  id          String    @id @default(uuid())
  sellerId    String    @unique
  seller      User      @relation(fields: [sellerId], references: [id])
  totalEarned Float     @default(0)
  pending     Float     @default(0)
  totalPaid   Float     @default(0)
  lastPaidAt  DateTime?
}
