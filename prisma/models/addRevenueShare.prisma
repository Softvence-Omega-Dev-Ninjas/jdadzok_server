model AdRevenueShare {
  id String @id @default(uuid())

  userId String
  user   User   @relation(fields: [userId], references: [id])

  month  Int
  year   Int
  amount Float

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@unique([userId, month, year])
  @@map("adRevenueShare")
}
